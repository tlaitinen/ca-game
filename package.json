  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.8",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "express": "^4.17.1",
    "nodemon": "^2.0.2",
