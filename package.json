    "typescript": "3.7.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "dev-server": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/server/index.ts",
